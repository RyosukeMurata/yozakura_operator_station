<?xml version="1.0"?>
<launch>
  <group ns="yozakura">
    <node name="joy_node" pkg="joy" type="joy_node">
        <remap from="joy" to="joy"/>
        <param name="dev" value="/dev/input/js0"/>
    </node>
    <node name="command_generator" pkg="operator_station" type="command_generator.py" output="screen">
        <remap from="yozakura_command" to="yozakura_command"/>
        <remap from="yozakura_state" to="yozakura_state"/>
        <remap from="yozakura_sensor_data" to="yozakura_sensor_data"/>
    </node>
    <node name="around_viewer" pkg="theta_around_viewer" type="around_viewer.py"/>
  </group>
</launch>
